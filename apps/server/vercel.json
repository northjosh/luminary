{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "bun run build:vercel",
  "installCommand": "cd ../.. && bun install",
  "functions": {
    "dist/node.js": {
      "runtime": "@vercel/node@3.2.35"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/node.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
